<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>哞哞管理后台</title>
	#parse('layout/thems/default/common/base.vm')
	<link href="${contentPath}/resources/assets/src/style/common/bootstrao-datetimepicker/bootstrap-datetimepicker.min.css" rel="stylesheet">
</head>
<body>

<!-- Header -->
#parse('layout/thems/default/header.vm')

	<div class="main-container" id="main-container">
			<script type="text/javascript">
				var pageNav="info_menu_list_${sysId}";
				try{ace.settings.check('main-container' , 'fixed')}catch(e){}
			</script>

			<div class="main-container-inner">
				<a class="menu-toggler" id="menu-toggler" href="#">
					<span class="menu-text"></span>
				</a>
				#parse('layout/thems/default/left.vm')

				<div class="main-content">
					<div class="breadcrumbs" id="breadcrumbs">
						<script type="text/javascript">
							try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
						</script>

						<ul class="breadcrumb">
							<li>
								<i class="icon-home home-icon"></i>
								<a href="#">首页</a>
							</li>
							<li class="active">商家管理</li>
						</ul><!-- .breadcrumb -->

						<div class="nav-search" id="nav-search">
							<form class="form-search">
								<span class="input-icon">
									<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
									<i class="icon-search nav-search-icon"></i>
								</span>
							</form>
						</div><!-- #nav-search -->
					</div>

					<div class="page-content">
						<div class="page-header">
							<h1>
								店铺列表
							</h1>
						</div><!-- /.page-header -->

						<div class="row">
							
							<div class="col-xs-12">
										<div class="table-header">
											店铺列表详情
										</div>
										<div class="table-responsive">
											<div role="grid" class="dataTables_wrapper" id="sample-table-2_wrapper">
                                                <div class="row">
                                                    <div class="col-sm-2">
														<input  placeholder="开始日期"  type="text" data-date-format="yyyy-mm-dd HH:mm:ss" id="id-date-picker-3" class="form-control date-picker">
													</div>
													<div class="col-sm-2">												
														<input  placeholder="结束日期"  type="text" data-date-format="yyyy-mm-dd HH:mm:ss" id="id-date-picker-4" class="form-control date-picker">
													</div>
													<div class="col-sm-3">
													<div class="input-group m-b">
														<div class="input-group-btn">
															<button type="button" class="btn btn-white dropdown-toggle" role="button" data-toggle="dropdown" aria-expanded="false">
                                                               		 手机号 <span class="caret"></span>
                                                            </button>
															<ul class="dropdown-menu searchDropDown" role="menu">
                                                                <li data-hid="userPhone" class="active"><a href="javascript:void(0);">手机号</a></li>
                                                            </ul>
                                                        </div>
                                                        <input type="text" class="form-control" placeholder="Search" value="" id="searchValue">
														<span class="input-group-btn">
															<button class="btn btn-sm btn-primary" type="button" id="serchBtn" data-fild="userPhone">Go!</button>
														</span>
													</div>
                                                </div>
                                                </div>
												<table class="table table-striped table-bordered table-hover dataTable" id="sample-table-2" aria-describedby="sample-table-2_info" >
													<thead>
														<tr role="row">
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 80px;" >用户ID</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 80px;" >用户昵称</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 100px;" >用户手机号</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 100px;" >创建时间</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 120px;" >修改时间</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 80px;" >店铺状态</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 80px;" >店铺详细信息</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 120px;" >店铺简介</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 120px;" >备注</th>
															<th class="hidden-480" role="columnheader" tabindex="0"  rowspan="1" colspan="1" style="width: 50px;" >操作</th>
														</tr>
													</thead>
													<tbody id="theacertbody" role="alert" aria-live="polite" aria-relevant="all">
													</tbody>
												</table>
                                                <ul class="pagination">
                                                </ul>
											 <div/>
											 #foreach($data in $skillMap.entrySet())
												<input type="hidden" id="skillId${data.key}" value="${data.value.name}">
											 #end
											 <div>
									</div>
						</div><!-- /.row -->
					</div><!-- /.page-content -->
				</div><!-- /.main-content -->
				
		
			</div><!-- /.main-container-inner -->

			<a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
				<i class="icon-double-angle-up icon-only bigger-110"></i>
			</a>
		</div>
#parse('layout/thems/default/setting.vm')

<!-- ========== CONTENT END ========== -->

<!-- ========== FOOTER START ========== -->
#parse('layout/thems/default/footer.vm')
<!-- ========== FOOTER END ========== -->
<!-- 配置文件 -->

<script type="text/javascript">
 	seajs.use("business-shop-list");
</script>
<!-- Modal -->
 <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">店铺内容清单</h4>
                    </div>
                    <div class="modal-body">
                        <form role="form" class="form-horizontal">
                            <table class="table table-bordered" id="sample-table-2" >
                                <thead>
                                <tr role="row">
                                	<th class="hidden-480 sorting" tabindex="0"  rowspan="1" colspan="1" style="width: 150px;" >店铺属性</th>
                                    <th class="hidden-480 sorting" tabindex="0"  rowspan="1" colspan="1" style="width: 150px;" >配图</th>
                                    <th class="hidden-480 sorting" tabindex="0"  rowspan="1" colspan="1" style="width: 150px;" >链接地址</th>
                                </tr>
                                </thead>
                                <tbody id="refoundtbody" role="alert" aria-live="polite" aria-relevant="all">

                                </tbody>
                            </table>
                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="examineModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">审核</h4>
                    </div>
                    <div class="modal-content">
                    	<textarea rows="5" cols="6" style="width: 100%" id="pass"></textarea>
                    </div>
                    <div class="modal-footer">
                        <input type="hidden" id="examineId" value="0" />
                        <button type="button" class="btn btn-success">通过</button>
                        <button type="button" class="btn btn-danger">不通过</button>
                    </div>
                </div>
            </div>
        </div>
</body>
</html>